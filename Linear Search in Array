#include <stdio.h>
int main() {
    int n, a[50], key, found = 0;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    for(int i=0; i<n; i++)
        scanf("%d", &a[i]);
    printf("Enter element to search: ");
    scanf("%d", &key);
    for(int i=0; i<n; i++) {
        if(a[i] == key) {
            found = 1;
            break;
        }
    }
    if(found)
        printf("Element found");
    else
        printf("Element not found");

    return 0;
}
